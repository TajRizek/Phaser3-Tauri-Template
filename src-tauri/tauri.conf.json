    "devUrl": "http://localhost:5500",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "category": "Game",
    "copyright": "",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
